<?xml version="1.0" encoding="utf-8"?>
<manifest package="memoizrlabs.com.composable_styles"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".App"
        android:theme="@style/AppTheme">
        <activity android:name="memoizrlabs.com.composable_styles.app.view.UsersActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="memoizrlabs.com.composable_styles.app.view.FooActivity"/>
        <activity android:name="memoizrlabs.com.composable_styles.app.view.SettingsActivity"/>
    </application>

</manifest>